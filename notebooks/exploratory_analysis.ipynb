    "## 2. Fraud Distribution Analysis"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "fraud-overview",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Fraud distribution\n",
    "fraud_counts = df['is_fraud'].value_counts()\n",
    "fraud_pct = df['is_fraud'].value_counts(normalize=True) * 100\n",
    "\n",
    "fig, (ax1, ax2) = plt.subplots(1, 2, figsize=(15, 5))\n",
    "\n",
    "# Count plot\n",
    "fraud_counts.plot(kind='bar', ax=ax1, color=['green', 'red'])\n",
    "ax1.set_title('Fraud Distribution (Count)')\n",
    "ax1.set_xlabel('Is Fraud')\n",
    "ax1.set_ylabel('Count')\n",
    "ax1.set_xticklabels(['Normal', 'Fraud'], rotation=0)\n",
    "\n",
    "# Percentage pie chart\n",
    "ax2.pie(fraud_pct.values, labels=['Normal', 'Fraud'], autopct='%1.2f%%', \n",
    "        colors=['green', 'red'])\n",
    "ax2.set_title('Fraud Distribution (Percentage)')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "print(f\"Normal transactions: {fraud_counts[0]:,} ({fraud_pct[0]:.2f}%)\")\n",
    "print(f\"Fraudulent transactions: {fraud_counts[1]:,} ({fraud_pct[1]:.2f}%)\")"
   ]
  },
